#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;


void main ()
{int a, b, div;
cout<<"ingrese a: ";
cin>>a;
cout<<"ingrese b: ";
cin>>b;
if(a%b==0)
{
   div=a/b;
cout<<"el resultado final es: "<< div;
}
getch();
}
